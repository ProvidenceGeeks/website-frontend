{
  "name": "website-frontend",
  "version": "0.1.0",
  "description": "UI frontend repository for the Providence Geeks website. Built with React and webpack.",
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run lint && webpack --config ./webpack.config.prod.js -p",
    "develop": "webpack-dev-server --open --config ./webpack.config.develop.js",
    "lint": "eslint *.js",
    "serve": "npm run build && http-server ./build -o -p 9000"
  },
  "repository": {
    "url": "https://github.com/ProvidenceGeeks/website-frontend.git",
    "type": "git"
  },
  "author": "pholyoke@kenzan.com",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0"
  }
}