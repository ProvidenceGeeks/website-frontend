{
  "name": "react-webpack-stack",
  "version": "1.0.0",
  "description": "A sample project showcasing Yarn, webpack and TypeScript with React for the Linux.com article series.",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && webpack --config ./webpack.config.prod.js -p",
    "clean": "rimraf ./build && rimraf ./reports && mkdir ./reports && mkdir ./reports/test-results",
    "develop": "webpack-dev-server --open --config ./webpack.config.develop.js",
    "lint": "eslint *.js src/**/*.spec.jsx",
    "serve": "npm run build && http-server ./build -o -p 9000",
    "start": "npm run serve",
    "test": "jest --coverage --config ./jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kenzanlabs/react-webpack-stack.git"
  },
  "keywords": [
    "kenzanlabs"
  ],
  "author": "rhernandez@kenzan.com",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "^4.7.0",
    "jsonp": "^0.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "3.0.2",
    "ts-md5": "^1.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.2",
    "@types/jest": "^20.0.2",
    "@types/jsonp": "^0.2.0",
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/react-router": "3.0.2",
    "awesome-typescript-loader": "^3.2.1",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "http-server": "^0.10.0",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "node-sass": "^4.5.3",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0"
  }
}
